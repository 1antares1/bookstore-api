{
    "name": "bookstore",
    "version": "1.0.0",
    "description": "GBH Project",
    "authors": [
        "José G. Ramírez"
    ],
    "license": "GPL3",
    "homepage": "https://gbh.com.do/",
    "repository": {
        "type": "git",
        "url": ""
    },
    "scripts": {
        "start": "npm run server-start",
        "start:prod": "npm run build:prod && npm run server:prod",
        "start:hmr:ssl": "ng serve --hmr -e=hmr --proxy-config proxy.conf.js --ssl 1 --ssl-key \"certs/gbh.com.do.key.pem\" --ssl-cert \"certs/gbh.com.do.key.pem/gbh.com.do.cert.pem\"",
        "build": "tsc --project tsconfig.json --sourceMap",
        "build:prod": "tsc --project tsconfig.json",
        "clean:dist": "npm run rimraf -- dist",
        "clean:install": "npm set progress=false && npm install",
        "clean": "npm cache clean --force && npm run rimraf -- node_modules doc coverage dist compiled dll",
        "cls": "clear && printf '\\e[3J'",
        "install:prod": "npm update && npm install --production && node node_modules/node-sass/scripts/install.js",
        "docker": "docker",
        "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/",
        "docs:compodoc": "compodoc -p tsconfig.json",
        "docs:compodoc:serve": "compodoc -p tsconfig.json -s",
        "docs:compodoc:serve:watch": "compodoc -p tsconfig.json -s -w",
        "node": "node",
        "postversion": "git push && git push --tags",
        "rimraf": "rimraf",
        "server:prod": "set NODE_ENV=production && node app.js",
        "server-start": "node app.js",
        "tslint": "tslint"
    },
    "private": true,
    "dependencies": {
        "body-parser": "^1.18.3",
        "cookie-parser": "1.4.3",
        "core-js": "2.5.7",
        "crypto-js": "^3.1.9-1",
        "ecstatic": "3.2.1",
        "errorhandler": "1.5.0",
        "firebase": "5.2.0",
        "helmet": "3.12.1",
        "https": "1.0.0",
        "jquery": "^3.3.1",
        "jsdom": "^11.11.0",
        "lodash": "4.17.10",
        "method-override": "2.3.10",
        "moment": "^2.22.2",
        "morgan": "1.9.0",
        "npm": "6.1.0",
        "query-string": "6.1.0",
        "request": "^2.87.0",
        "request-promise": "4.2.2",
        "routes": "2.1.0",
        "rxjs": "6.2.1",
        "rxjs-compat": "6.2.1",
        "save": "2.3.2",
        "serve-favicon": "2.5.0",
        "ssl-root-cas": "1.2.5",
        "tsd": "0.6.5",
        "uuid": "3.3.2",
        "xmlbuilder": "10.0.0"
    },
    "devDependencies": {
        "@types/bluebird": "3.5.21",
        "@types/body-parser": "1.17.0",
        "@types/core-js": "2.5.0",
        "@types/jquery": "^3.3.4",
        "@types/jsdom": "^11.0.6",
        "@types/lodash": "4.14.110",
        "@types/node": "10.5.2",
        "@types/query-string": "6.1.0",
        "@types/request": "2.47.1",
        "@types/request-promise": "4.1.41",
        "@types/xmlbuilder": "0.0.32",
        "codelyzer": "4.4.2",
        "source-map": "0.7.3",
        "ts-node": "7.0.0",
        "tslint": "5.10.0",
        "typescript": "2.7.2"
    }
}
